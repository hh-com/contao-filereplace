{
    "name": "hh-com/contao-filereplace",
    "type": "contao-bundle",
    "description": "Replace Files in Contao backend",
	"keywords": ["filemanager", "files"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "contao/core-bundle": "^5.3"
    },
    "extra": {
        "contao-manager-plugin": "Hhcom\\ContaoFileReplace\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Hhcom\\ContaoFileReplace\\": "src/"
        },
		"classmap": [
            "contao"
		],
		"exclude-from-classmap": [
			"contao/config",
            "contao/templates/",
			"contao/languages"
		]
    }
}